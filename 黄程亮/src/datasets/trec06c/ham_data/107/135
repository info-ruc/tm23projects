<script language="javascript">
	//函数名：chkemail
	//功能介绍：检查是否为Email Address
	//参数说明：要检查的字符串
	//返回值：0：不是  1：是 
	function chkemail(a)
	{ var i=a.length;
	 var temp = a.indexOf('@');
	 var tempd = a.indexOf('.');
	 if (temp > 1) {
	  if ((i-temp) > 3){
	   
		if ((i-tempd)>0){
		 return 1;
		}
	   
	  }
	 }
	 return 0;
	}

	//函数名：chkspc
	//功能介绍：检查是否含有空格
	//参数说明：要检查的字符串
	//返回值：0：是  1：不是
	function chkspc(a)
	{
	 var i=a.length;
	 var j = 0;
	 var k = 0;
	 while (k<i)
	 {
	  if (a.charAt(k) != " ")
	   j = j+1;
	  k = k+1;
	 }
	 if (j==0)
	 {
	  return 0;
	 }
	 
	 if (i!=j)
	 { return 0; }
	 else
	 {
	  return 1;
	 }
	}

	//函数名：fucCheckNUM
	//功能介绍：检查是否为数字
	//参数说明：要检查的数字
	//返回值：1为是数字，0为不是数字
	function fucCheckNUM(NUM)
	{
	 var i,j,strTemp;
	 strTemp="0123456789";
	 if ( NUM.length== 0)
	  return 0;
	 for (i=0;i<NUM.length;i++)
	 {
	  j=strTemp.indexOf(NUM.charAt(i)); 
	  if (j==-1)
	  {
	  //说明有字符不是数字
	   return 0;
	  }
	 }
	 //说明是数字
	 return 1;
	}



function saveCheck()
{
	
	if (form1.com_name.value=="") {
		window.event.returnValue = false ;
		alert("Please enter company name!") ;
		form1.com_name.focus();
		return;
	}
	if (form1.com_address.value=="") {
		window.event.returnValue = false ;
		alert("Please enter company address！") ;
		form1.com_address.focus();
		return;
	}
		if (form1.email.value=="") {
		window.event.returnValue = false ;
		alert("Please enter a valid email address！") ;
		form1.email.focus();
		return;
	}
	if (chkemail(form1.email.value) == 0)
		{
			alert("Please enter a valid email address！");
			form1.email.focus();
			return false;
		}
		
	
}




//本页表格有多少行
	var currRNum = 1 ;
	var maxrow;


	function insertTabRow(tab, cNum,maxrow) {
		if (currRNum < maxrow)
		{
  		var row = tab.insertRow() ;

		/* 列名保存在数组中
		*/
	currRNum ++ ;
		for ( var i = 0 ; i < cNum ; i ++) {
                      
			row.insertCell().innerHTML ="<font color=#666666>Name: <input name='name"+currRNum+"' type=text class=box size=10 > <font color=#CC3300>*</font> Sex : <font color=#666666> <input name='sex"+currRNum+"'  type=radio value=1 checked> Male <input type=radio name='sex"+currRNum+"'  value=0> Female </font> Position:   <input name='position"+currRNum+"' type=text size=16 class=box> </font> ";			
		}
			//每增加一行就加1
        document.all("num").value = currRNum;
		}

	}

	function deleteTabRow(tab) {
		var rNO = currRNum-1;
		if (rNO > 0)
		{
		currRNum--;
		document.all("num").value = currRNum;
		tab.deleteRow(rNO) ;
		}
	}

</script>
