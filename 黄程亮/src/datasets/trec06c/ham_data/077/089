function getUrlSubstring (startsWith, endsWith) {=0A=
	// get the position of the first string=0A=
	startPosition =3D document.location.href.indexOf(startsWith);=0A=
	=0A=
	// if the string is found...	=0A=
	if (startPosition !=3D -1) {=0A=
		// find the position of the second string starting after the first =
position=0A=
		endPosition =3D document.location.href.indexOf(endsWith, =
startPosition);=0A=
		// extract the substring between the two positions=0A=
		returnString =3D document.location.href.substring(startPosition, =
endPosition);=0A=
	} else { // if the string isn't found...=0A=
		returnString =3D "";=0A=
	}=0A=
	=0A=
	// return the substring=0A=
	return returnString;=0A=
}=0A=
=0A=
function readCookie (cookieName) {=0A=
=0A=
	raw_cookie =3D document.cookie;=0A=
	=0A=
	startPos =3D raw_cookie.indexOf(cookieName);=0A=
	=0A=
	if (startPos !=3D -1) {=0A=
	=0A=
		startPos =3D raw_cookie.indexOf("=3D", startPos) + 1;=0A=
		=0A=
		endPos =3D raw_cookie.indexOf(";", startPos);=0A=
		=0A=
		if (endPos =3D=3D -1) { endPos =3D raw_cookie.length }=0A=
		=0A=
		var cookieValue =3D raw_cookie.substring(startPos, endPos);=0A=
		=0A=
		return cookieValue=0A=
	=0A=
	} else {=0A=
	=0A=
		return "none";=0A=
		=0A=
	}=0A=
}=0A=
