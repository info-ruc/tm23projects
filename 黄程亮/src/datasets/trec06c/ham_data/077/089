	} else if (currSelect =3D=3D "advanced") {=0A=
		top.location =3D "/cgi-bin/redirect?url=3D%2Fsearch%2Fadvanced.htm";=0A=
	} else if (currSelect =3D=3D "null") {=0A=
		popup.selectedIndex =3D 0;=0A=
	} else {=0A=
		popup.form.QueryText.focus();=0A=
	}=0A=
}=0A=
=0A=
function queryValid (currForm) {=0A=
=0A=
var popupValue =3D =
currForm.collname[currForm.collname.selectedIndex].value;=0A=
	if (popupValue =3D=3D "pastIssues" || popupValue =3D=3D "advanced" || =
popupValue =3D=3D "null") {=0A=
		currForm.collname.selectedIndex =3D 0;=0A=
		currForm.collname.focus();=0A=
		return false;=0A=
	} else {=0A=
		if (currForm.elements['QueryText'].value =3D=3D "") {=0A=
			alert ("Please enter a search term");=0A=
			currForm.elements['QueryText'].focus();=0A=
			return false;=0A=
		} else {=0A=
			return true;=0A=
		}=0A=
	}=0A=
