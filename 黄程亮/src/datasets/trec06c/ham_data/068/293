<SCRIPT language=3DVBSCRIPT>
	dim Genie
	dim GenieID
	dim GenieACS
	dim GenieReady
	dim NotLocal
	dim sleepTime
	dim bAgentIntroduced
	sleepTime =3D 120000
	GenieID =3D "Genie"
	GenieACS =3D "Genie.acs"
	GenieSleep =3D window.setInterval("sleep()",sleepTime)
	Sub Window_OnLoad()
		bAgentIntroduced =3D false
		GenieReady =3D False
		If HaveAgent() Then
			On Error Resume Next
			AgentControl.Connected =3D True
			If Not LoadLocalAgent(GenieID, GenieACS) Then
			    Call SetGenieObject
			Else
			    NotLocal =3D True=20
			    AgentControl.Characters.Load "Genie", =
"http://agent.microsoft.com/agent2/chars/Genie/Genie.acf"
			    Call SetGenieObject
			    Window.Status =3D ""
			    GenieReady =3D True
			    Call AgentIntro
			    bAgentIntroduced =3D true
			End If
		end if
	End Sub
	Function LoadLocalAgent(ByVal CharID, ByVal CharACS)
	    On Error Resume Next
	    AgentControl.Characters.Load CharID, CharACS
	    If Error =3D 0 Then
	        LoadLocalAgent =3D True
	    Else
	        LoadLocalAgent =3D False
	    End If
	End Function
	Sub SetGenieObject()
	    On Error Resume Next
	    Set Genie =3D AgentControl.Characters(GenieID)
	    Genie.LanguageID =3D &H409
	End Sub
	Sub AgentControl_DragStart (ByVal CharacterID, ByVal Button, ByVal =
Shift, ByVal X, ByVal Y)
	    window.clearInterval(GenieSleep)
	    GenieSleep =3D window.setInterval("sleep()",sleepTime)
	End Sub
	Sub AgentControl_Click(ByVal CharacterID, ByVal Button, ByVal Shift, =
ByVal X, ByVal Y)
	    Genie.Show
	    Genie.StopAll "Move"
	    Genie.StopAll "Play"
	    Genie.StopAll "Speak"
	    Genie.Play "RestPose"
	    window.clearInterval(GenieSleep)
	    GenieSleep =3D window.setInterval("sleep()",sleepTime)
	End Sub
	Sub AgentControl_DblClick(ByVal CharacterID, ByVal Button, ByVal Shift, =
ByVal X, ByVal Y)
	    On Error Resume Next
	    Genie.StopAll
	    If Not GenieID.HasOtherClients Then
	        If Genie.Visible Then
	            Set HideReq =3D Genie.Hide()
	        Else
	            AgentControl.Characters.Unload GenieID
	        End If
	    End If
	End Sub
	Sub AgentControl_Command(ByVal UserInput)
	    On Error Resume Next
	    Select Case UserInput.Name
	    Case "ACO"
	        AgentControl.PropertySheet.Visible =3D True
	    End Select
	End Sub
	Sub AgentIntro()
	    On Error Resume Next
	    Genie.Commands.RemoveAll
	    Genie.Commands.Caption =3D "My Menu Name"
	    Genie.Commands.Add "ACO", "Advanced Character Options", "Advanced =
Character Options"
	    If NotLocal Then
	        Genie.Get "State", "Showing, Speaking, Moving, Wave"
	        Genie.Get "Animation", ""
	        Genie.Get "State", "Hiding"
	        GenieReady =3D True
	    End If
	    Genie.MoveTo window.screenleft + 325, window.screentop + 200
	    Genie.show
	    Genie.Play "Idle2_2"
	    Genie.Speak document.all.bob.innerText
	End Sub
	sub genieMouseOver(ID)
		if frmPostcard.genieinstalled.value =3D "yes" then
			if bAgentIntroduced then
			    If ID =3D 1 Then
			        Genie.speak document.frmPostcard.speak1.value
			    ElseIf ID =3D 2 Then
			        Genie.speak document.frmPostcard.speak2.value
			    End If
			end if
		end if
	End sub
	Sub document_unload()
	    If GenieReady Then
	        Genie.Hide
	    End If
	End Sub
	Sub sleep()
	    If GenieReady Then
	        Genie.MoveTo (Screen.Width - 100),(Screen.height - 150)
	        Genie.Play "Wave"
	    End If
	End Sub
</SCRIPT>
</BODY></HTML>

