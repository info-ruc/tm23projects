     -2401 <ms% 500> "+"/<ms% 2000> "L Res Ret: Ctr Stim 1" /
       <ms% 200> g "mask"/* "Mismatch  Stim 1" / <ms% 500> /;
     -2402 <ms% 500> "+"/<ms% 2000> "L Res Ret: Ctr Stim 2" /
       <ms% 200> g "mask"/* "Mismatch Stim 2" / <ms% 500> /;
     -2403 <ms% 500> "+"/<ms% 2000> "L Res Ret: Ctr Stim 3"/
