>>
>>+001 %100 "+"/%3"#####"/ %2"acres"/%3"#####"/* "?";
>>+002 * %0 "acres"/!y250 "cares";
>>+003 %100 "+"/%3"#####"/ %2"papal"/%3"#####"/* "?";
>>+004 * %0 "papal"/!y250 "appal";
>>+005 %100 "+"/%3"#####"/ %2"raise"/%3"#####"/* "?";
>>+006 * %0 "arise"/!y250 "raise";

                                                /"\
-jonathan (j.c.f.)                             \ /
