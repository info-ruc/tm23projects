E:\Data\Fetzer\Bs2m07o.bmp"/ <incic 1>;
-1011<umpr> <umnr>  <mnr -Bit4> /%0    /%30  g" E:\Data\Fetzer\SelfValence.bmp"
/  %20  g" 
E:\Data\Fetzer\crosshair.bmp"/ %10  o2    /%128   g *" E:\Data\Fetzer\9180.bmp"/%6
  g" 
E:\Data\Fetzer\Bs2m06f.bmp"/ <incic 1>;
+1012<umpr> <umnr>  <mpr +Bit2> /%84   /%30  g" E:\Data\Fetzer\SelfValence.bmp"
/  %20  g" 
E:\Data\Fetzer\crosshair.bmp"/ %10  o1    /%128   g *" E:\Data\Fetzer\2810.bmp"/%6
  g" 
E:\Data\Fetzer\Bs2m02o.bmp"/ <incic 1>;

!Set 2;
8803 <emit 1> ;
8804  <fd 60> "Ready?"/ <fd 60> "Set2" ;
8805 <set 1, 0> d60/;

. . . . etc. for 2 or 7 more blocks.

9901 <emit 1>;
9902 <fd 60> "Thank You" <end>;







