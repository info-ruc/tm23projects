+00000005  * "GREEN" <msfd 500>/ <msfd 1500>/<set c1 = c1+LastRT>;
+00000006  * "BLUE" <msfd 500>/ <msfd 2000>/<set c1 = c1+LastRT>;
+00000007  * "RED" <msfd 500> / <msfd 1500>/<set c1 = c1+LastRT>;
