+97 <nfb 1><ntl 0><fd 603><jpg> "images/scene512" / <fd 603><jpg>
"images/scene412" / <fd 603><jpg> "images/scene212" / * <fd 1> /;
-5100 <mpr +/><mnr +z><nfb 0><ntl><cfb "Correct"><wfb "Incorrect"><ms%
500> "-+-" / * <msfd 1000><jpg> "images/s5i11t0" /;
