	 var pubNameCheck =3D "The Wall Street Journal Interactive Edition";=0A=
	if (pubNameCheck =3D=3D "The Wall Street Journal Interactive Edition")=0A=
	{=0A=
		var pubName=3D escape("wsj.com");=0A=
	}=0A=
	else if (pubNameCheck =3D=3D "Barron's Online")=0A=
	{=0A=
		var pubName=3D escape("barrons.com");=0A=
	}=0A=
	else=0A=
	{=0A=
		var pubName=3D escape("newswires");=0A=
	}=0A=
=0A=
	var url =3D "https://s100.copyright.com/Clients/wsj_com/FairUse.jsp";=0A=
	var location =3D url + "?PublisherName=3D"	+ pubName + "&ContentID=3D" =
+ contentID + "&orderReset=3Dtrue" + "&Title=3D" + title + "&Author=3D" =
+ author + "&CreditLine=3D" + creditline + "&PublicationDate=3D" + =
pubDate;=0A=
	PopUp =3D window.open(location, 'reprints', =
'location=3Dno,toolbar=3Dno,directories=3Dno,status=3Dno,menubar=3Dno,scr=
ollbars=3Dyes,resizable=3Dyes,width=3D515,height=3D440');=0A=
}=0A=
