function checkForm()
{
 document.frm1.subjectid.checked = true;
 if(document.frm1.FromName.value=="") {
 alert("กรุณากรอกชื่อของคุณ");
 return false;
 }
 if(document.frm1.Tel.value=="") {
 alert("กรุณากรอกเบอร์โทรศัพท์");
 return false;
 }
 if(document.frm1.FromEmail.value=="") {
 alert("กรุณากรอกอีเมล์");
 return false;
 }
}
